using System;  
  public class SwapExample  
   {  
     public static void Main(string[] args)  
      {  
       int  a=1, b=2;            
        
       a=a+b; //a=15 (5+10)      
       b=a-b; //b=5 (15-10)      
       a=a-b; //a=10 (15-5)   
       Console.Write("The value of the 2nd variable :"+b);   
       Console.Write("The value of the 1st variable :"+a);
      }  
  }  
